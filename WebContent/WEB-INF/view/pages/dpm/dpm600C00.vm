## 【検索】品目カテゴリ選択処理
function changeProd() {

    var category = document.getElementById("prodCategory").value;
	changeProdCategoryList(document.dpm600C00Form);

	## 医薬-ワクチン選択項目表示切替 前準備
	## jrnsの場合は、品目のカテゴリにて、表示切替をする
	if(category == '$jrnsCode'){
		let options = document.getElementById('prodCode').options;
		let selectedprodCode = document.getElementById('prodCode').value;
		for(var i = 0; i < options.length ;i++ ){
			if (options[i].value == selectedprodCode ) {
				category = options[i].getAttribute('prodcategory');
				break;
			}
		}
	}

	switchOptionVisibility(category);
}

## 医薬-ワクチン選択項目表示切替 品目切り替え時
function switchOptionVisibilityByChangeProd(){
	let options = document.getElementById('prodCode').options;
	let selectedprodCode = document.getElementById('prodCode').value;
	for(var i = 0; i < options.length ;i++ ){
		if (options[i].value == selectedprodCode ) {
			category = options[i].getAttribute('prodcategory');
			break;
		}
	}
	switchOptionVisibility(category);
}

## 医薬-ワクチン選択項目表示切替
## 重点先・一般先/市区町村」を表示するか、「対象区分」を表示するか
function switchOptionVisibility(category){
	## ワクチンの場合
    if(category == "$vaccineCode"){
        ## 重点先/一般先
        document.getElementById("activityType_tr").style.display=''
        ## 市区町村
        document.getElementById("addr_tr").style.display=''
