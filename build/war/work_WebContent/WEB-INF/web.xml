    <init-param>
      <param-name>chainConfig</param-name>
      <param-value>/WEB-INF/conf/struts/chain-config.xml</param-value>
    </init-param>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>action</servlet-name>
    <url-pattern>/app/*</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>velocity</servlet-name>
    <servlet-class>jp.co.takeda.web.cmn.velocity.DpVelocityServlet</servlet-class>
