    <env-entry-type>java.lang.Integer</env-entry-type>
    <env-entry-value>1999102</env-entry-value>
  </env-entry>
  <env-entry>
    <env-entry-name>dp/operatorUserId2</env-entry-name>
    <env-entry-type>java.lang.Integer</env-entry-type>
    <env-entry-value>1999503</env-entry-value>
  </env-entry>
  <env-entry>
    <env-entry-name>dp/releaseNumber</env-entry-name>
    <env-entry-type>java.lang.String</env-entry-type>
    <env-entry-value>03_001</env-entry-value>
  </env-entry>
  <jsp-config>
    <jsp-property-group>
      <description>JSPConfiguration</description>
      <display-name>JSPConfiguration</display-name>
      <url-pattern>*.jsp</url-pattern>
      <el-ignored>false</el-ignored>
      <page-encoding>UTF-8</page-encoding>
      <scripting-invalid>false</scripting-invalid>
    </jsp-property-group>
  </jsp-config>
